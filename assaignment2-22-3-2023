#include<stdio.h>
main()
{
	int n=1,sum1=0,sum2=0,d;
	while(n<=10)
	{
		printf("%d \n",n);
		if(n%2==0)
		{
			sum1=sum1+n;
		}
		else
		{
			sum2=sum2+n;
		}
        n++;
    }
    printf("even sum=%d\t",sum1);
    printf("odd sum=%d\n",sum2);
    d=sum1-sum2;
    printf("difference=%d",d);
    
}
