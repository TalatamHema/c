#include<stdio.h>
#include<conio.h>
void main()
{
	int n,i=1,sum1=0,sum2=0;
	printf("enter value of n\n");
	scanf("%d",&n);
	while(i<=n)
	{

		if(i%5==0)
		{
			printf("%d\n",i);
    	if(i%2==0)
		{
			sum1=sum1+i;
		}
		else
		{
			sum2=sum2+i;
		}
     	}
        i++;
        
    }
    printf("even sum=%d\t",sum1);
    printf("odd sum=%d",sum2);

	
}
